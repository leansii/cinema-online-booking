    "pinia": "^2.1.7",
    "vue": "^3.5.22",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@testing-library/vue": "^7.0.0",
    "@types/node": "^24.6.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "@vue/test-utils": "^2.4.6",
    "@types/jsdom": "^21.1.7",
    "jsdom": "^24.1.2",
    "openapi-typescript-codegen": "^0.27.0",
    "typescript": "~5.9.3",
    "vite": "npm:rolldown-vite@7.1.14",
    "vitest": "^1.6.0",
    "vue-tsc": "^3.1.0"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.1.14"
  }
